<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <padding
                android:bottom="0dp"
                android:right="0dp"
                android:left="0dp"
                android:top="0dp" />
            <solid android:color="#99A69D9D"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="@color/white"/>
            <corners android:radius="10dp"/>
            <stroke android:color="#AEA5A5"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>